{
  "name": "nearby-cities",
  "version": "1.1.1",
  "description": "Give me your coordinates and I'll tell you where the nearest cities are. Works offline.",
  "main": "index.js",
  "scripts": {
    "build": "npm install --prefix=functions/search",
    "test": "mocha && standard",
    "lint": "standard --fix"
  },
  "repository": "https://github.com/zeke/nearby-cities",
  "keywords": [
    "geolocation",
    "latitude",
    "longitude",
    "placename",
    "placenames",
    "geo",
    "gis",
    "location",
    "city",
    "state",
    "country",
    "zip",
    "county"
  ],
  "author": "Zeke Sikelianos <zeke@sikelianos.com> (http://zeke.sikelianos.com)",
  "license": "MIT",
  "devDependencies": {
    "mocha": "^3.2.0",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "all-the-cities": "git+https://github.com/hulab/all-the-cities.git",
    "geokdbush": "^1.0.2",
    "kdbush": "^1.0.1"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
